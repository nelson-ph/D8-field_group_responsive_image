<?php

/**
 * @file
 * Contains field_group_responsive_image.module.
 */

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_help().
 */
function field_group_responsive_image_help($route_name, RouteMatchInterface $route_match) {
  switch ($route_name) {
    // Main module help for the field_group_responsive_image module.
    case 'help.page.field_group_responsive_image':
      $output = '';
      $output .= '<h3>' . t('About') . '</h3>';
      $output .= '<p>' . t('Field group responsive image') . '</p>';
      return $output;

    default:
  }
}
